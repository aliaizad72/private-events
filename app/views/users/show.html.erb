<h1>Here is your events, <%= @user.name %>!</h1>

<ol>
  <% @user.created_events.each do |event| %>
    <li>
      <ul>
        <li>
          Event name: <%= event.name %>
        </li>
        <li>
          Created by: <%= event.creator.email %>
        </li>
      </ul>
    </li>
  <% end %>
</ol>